   <xsd:element name="NumFattura" type="xsd:string" /> 
   <xsd:element name="DataFattura" type="xsd:date" /> 
   <xsd:element name="Descrizione" type="xsd:string" /> 
   <xsd:element name="TotaleFattura" type="xsd:decimal" /> 
   <xsd:element name="Imponibile" type="xsd:decimal" /> 
   <xsd:element name="DataOraPeriodo" type="xsd:string" /> 
  </xsd:sequence> 
 </xsd:complexType> 
</xsd:schema>