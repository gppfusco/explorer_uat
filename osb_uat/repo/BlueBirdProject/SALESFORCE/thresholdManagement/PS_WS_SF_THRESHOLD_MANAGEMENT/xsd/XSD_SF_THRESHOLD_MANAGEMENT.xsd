		<xs:sequence>
			<xs:element name="assetId" type="xs:string"/>
			<xs:element name="exceededThresholdDate" type="xs:date"/>
			<xs:element name="exceededThresholdType" type="xs:string"/>
			<xs:element name="sendTranscomm" type="xs:string"/>
			<xs:element name="orderNumber" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="responseUpdateAlertNotificationType">
		<xs:sequence>
			<xs:element name="resultCode" type="xs:string"/>
			<xs:element name="resultDescription" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>