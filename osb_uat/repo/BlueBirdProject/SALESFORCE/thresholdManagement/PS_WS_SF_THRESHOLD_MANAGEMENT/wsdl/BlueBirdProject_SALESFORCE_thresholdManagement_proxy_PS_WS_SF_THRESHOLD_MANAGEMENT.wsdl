<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="WSDL_SF_THRESHOLD_MANAGEMENT"
     targetNamespace="http://www.skytv.it/mdw/service/WSDL_SF_THRESHOLD_MANAGEMENT"
     xmlns:tns="http://www.skytv.it/mdw/service/WSDL_SF_THRESHOLD_MANAGEMENT"
     xmlns:inp1="http://www.skytv.it/mdw/data"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://www.skytv.it/mdw/data" schemaLocation="../xsd/XSD_SF_THRESHOLD_MANAGEMENT.xsd"/>
            <xsd:import namespace="http://www.skytv.it/mdw/data" schemaLocation="../../../../../EnterpriseObjects/CommonEntities/SoapHeaderSKY.xsd"/>
       </xsd:schema>
    </wsdl:types>
    <wsdl:message name="requestMessage">
        <wsdl:part name="part_requestUpdateAlertNotification" element="inp1:requestUpdateAlertNotification"/>
    </wsdl:message>
    <wsdl:message name="replyMessage">
        <wsdl:part name="part_responseUpdateAlertNotification" element="inp1:responseUpdateAlertNotification"/>
    </wsdl:message>
    <wsdl:message name="headerMessage">
        <wsdl:part name="part_header" element="inp1:Header"/>
    </wsdl:message>

    <wsdl:portType name="ptt_WSDL_SF_THRESHOLD_MANAGEMENT">
        <wsdl:operation name="execute_UpdateAlertNotification">
            <wsdl:input message="tns:requestMessage"/>
            <wsdl:output message="tns:replyMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="bind_WSDL_SF_THRESHOLD_MANAGEMENT" type="tns:ptt_WSDL_SF_THRESHOLD_MANAGEMENT">
        <soap:binding transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="execute_UpdateAlertNotification">
            <soap:operation style="document" soapAction="execute_UpdateAlertNotification"/>
            <wsdl:input>
                <soap:body use="literal" namespace="http://www.skytv.it/mdw/service/WSDL_SF_THRESHOLD_MANAGEMENT"/>
                <soap:header message="tns:headerMessage" part="part_header" use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" namespace="http://www.skytv.it/mdw/service/WSDL_SF_THRESHOLD_MANAGEMENT"/>
                <soap:header message="tns:headerMessage" part="part_header" use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
</wsdl:definitions>