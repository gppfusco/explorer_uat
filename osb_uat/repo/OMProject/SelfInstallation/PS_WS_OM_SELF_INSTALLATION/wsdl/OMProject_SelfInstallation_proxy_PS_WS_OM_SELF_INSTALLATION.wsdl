			<wsdl:input>
				<soap:body parts="part_requestActivationContract" use="literal"/>
				<soap:header message="tns:header_message" part="part_header" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body parts="part_responseActivationContract" use="literal"/>
