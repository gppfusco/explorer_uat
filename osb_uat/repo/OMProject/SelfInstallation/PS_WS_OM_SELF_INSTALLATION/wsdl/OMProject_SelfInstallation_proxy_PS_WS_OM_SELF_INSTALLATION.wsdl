<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:tns="http://www.skytv.it/mdw/service/OM_SELF_INSTALLATION" xmlns:inp1="http://www.skytv.it/mdw/data" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" name="WS_OM_SELF_INSTALLATION" targetNamespace="http://www.skytv.it/mdw/service/OM_SELF_INSTALLATION">
	<wsdl:types>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://www.skytv.it/mdw/data" schemaLocation="../xsd/XSD_OM_SELF_INSTALLATION.xsd"/>
			<xsd:import namespace="http://www.skytv.it/mdw/data" schemaLocation="../../../../EnterpriseObjects/CommonEntities/SoapHeaderSKY.xsd"/>
		</xsd:schema>
	</wsdl:types> 
	<wsdl:message name="requestActivationContract_message">
		<wsdl:part name="part_requestActivationContract" element="inp1:requestActivationContract"/>
	</wsdl:message>
	<wsdl:message name="responseActivationContract_message">
		<wsdl:part name="part_responseActivationContract" element="inp1:responseActivationContract"/>
	</wsdl:message>
	<wsdl:message name="header_message">
		<wsdl:part name="part_header" element="inp1:Header"/>
	</wsdl:message>
	<wsdl:portType name="OM_SELF_INSTALLATION_ptt">
